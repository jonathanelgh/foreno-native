{
  "expo": {
    "name": "Föreno",
    "slug": "foreno-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "forenonative",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.foreno.native",
      "buildNumber": "6",
      "infoPlist": {
        "NSCameraUsageDescription": "Föreno behöver åtkomst till kameran för att ta bilder till information och profilbilder.",
        "NSPhotoLibraryUsageDescription": "Föreno behöver åtkomst till ditt fotobibliotek för att välja bilder till information och profilbilder.",
        "NSPhotoLibraryAddUsageDescription": "Föreno behöver tillgång för att spara bilder till ditt fotobibliotek.",
        "NSUserNotificationsUsageDescription": "Föreno vill skicka notiser om ny information och kommande aktiviteter.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.foreno.native"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/foreno-icon.png",
          "imageWidth": 120,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cb65ddda-a319-49f8-ad26-26ff23aae1e8"
      }
    },
    "owner": "jonathanelgh"
  }
}
